{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "Debug",
      "Host": "Error",
      "Function": "Debug",
      "Host.Aggregator": "Debug",
      "Host.Results": "Warning",
      "Azure": "None",
      "Worker": "Warning",
      "Microsoft": "Warning"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  },
  "extensions": {
    "serviceBus": {
      "prefetchCount": 1,
      "messageHandlerOptions": {
        "maxConcurrentCalls": 1
      }
    }
  },
  "telemetryMode": "openTelemetry"
}